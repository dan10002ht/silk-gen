{
  "name": "thuyen-silk-backend",
  "version": "1.0.0",
  "description": "Backend for Thuyen Silk inventory management system",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "dev:app": "CRON_STANDALONE=false nodemon src/index.js",
    "dev:cron": "CRON_STANDALONE=true nodemon src/cron/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "example:pubsub": "node src/examples/pubSubExample.js",
    "cron": "CRON_STANDALONE=true node src/cron/server.js",
    "build": "babel src -d dist",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.6.13",
    "sequelize": "^6.37.1",
    "winston": "^3.11.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.19",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.2.5",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4"
  }
}
