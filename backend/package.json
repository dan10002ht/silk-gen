{
  "name": "thuyen-silk-backend",
  "version": "1.0.0",
  "description": "Backend for Thuyen Silk inventory management system",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
